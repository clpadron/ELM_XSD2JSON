                    "type": "string",
                    "examples": ["eyJhbGciOiJSUzI1NiIsImI2NCI6ZmFsc2UsImNyaXQiOlsiYjY0Il19..TCYt5X"],
                    "default": ""
                },
                "proofValue": {
                    "type": "string",
                    "examples": ["BD21J4fdlnBvBA+y6D...fnC8Y="],
                    "default": ""
                }
