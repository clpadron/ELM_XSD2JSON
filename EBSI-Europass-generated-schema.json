        "mailBox_type": {
            "type": "object",
            "properties": {
                "@uri": {
                    "type": "string",
                    "examples": ["mailto:leaskywalker@gmail.com"],
                    "default": ""
                }
            }
        },
        "credentialSubject_type": {
            "type": "object",
            "properties": {
                "@id": {
                    "type": "string",
                    "examples": ["did:essif:123121"],
                    "default": ""
