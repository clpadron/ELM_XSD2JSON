                "@lang": {
                    "type": "string",
                    "examples": ["en"],
                    "default": ""
                },
                "#text": {
                    "type": "string",
                    "examples": ["Level 7"],
                    "default": ""
                }
            }
        },
        "targetName_type": {
            "type": "object",
            "properties": {
                "text": {"$ref": "#/definitions/targetName2_type"}
            }
