                "currentFamilyName": {
                    "type": "string",
                    "examples": ["Skywalker"],
                    "default": ""
                },
                "currentGivenName": {
                    "type": "string",
                    "examples": ["Lea"],
                    "default": ""
                },
                "dateOfBirth": {
                    "type": "string",
                    "format": "date",
                    "examples": ["2021-02-15"],
                    "default": ""
                },
                "mailBox": {"$ref": "#/definitions/mailBox_type"}
            }
        },
        "titlet2_type": {
            "type": "object",
            "properties": {
                "@content-type": {
                    "type": "string",
